<?xml version="1.0" ?>
<package>
<comment>
    QSort tests.
</comment>

<job id="QSortTest">
   <script language="VBScript" src="qsort.vbs" />
   <script language="VBScript">
<![CDATA[
option explicit

function TestArray()
    TestArray = Array(4,1,9,2,7)
end function

]]>
   </script>
   <script language="JScript">
<![CDATA[
function dump(arr){
    for (i = 0; i <= arr.ubound(); i++){
        WScript.StdOut.Write(arr.getItem(i));
    }
    WScript.Stdout.WriteLine();
}

var q = make_qsort();
var a;
dump(VBArray(TestArray()))

a  = q.Sort(VBArray(TestArray()))
dump(a)

q.Order = -1
a  = q.Sort(VBArray(TestArray()))
dump(a)

]]>
   </script>
</job>

</package>
